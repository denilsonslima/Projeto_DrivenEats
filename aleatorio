    const prato = document.querySelector(".borda1 h1")
    comida = prato.innerHTML;
    const prato2 = document.querySelector(".borda1>div>h2")
    preco = prato2.innerHTML;
    const res = preco.replace("R$ ", "")
    valor1 = res.replace(",", ".")
    final1 = Number(valor1)


